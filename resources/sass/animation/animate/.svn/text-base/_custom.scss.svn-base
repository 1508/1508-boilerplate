@include keyframes(delayedFade) {
  0%{
    max-height:0;
    opacity:0;
  }
  25%{
    max-height:0;
    opacity:0;    
  }
  50% {
    max-height:150px;
    opacity:0;
  }
  75% {
    opacity:1;    
    max-height:150px;   
  }
  100% {
    opacity:1;    
    max-height:150px;
  }
}




@include keyframes(displayNew) {
  0%{
    opacity:0;
  }
  25%{
    opacity:0;    
  }
  50% {
    opacity:1;    
  }
  100% {
    opacity:1;    
  }
}

@include keyframes(markCount) {
  0%{
  	background:transparent;
  }
  25% {
  	background:transparent;
  }
  37.5% {
  	background:#00793c;
  }  
  50% {
  	background:transparent;
  }
  62.5% {
  	background:#00793c;
  }
  75% {
  	background:transparent;
  }
  100% {
  	background:transparent;
  }
}

@include keyframes(dropdownFadeInDown) {
  0% {
  	margin-top:-5px;
    opacity: 0; }
  100% {
  	margin-top:15px;
    opacity: 1; } }

@include keyframes(dropdownFadeOutDown) {
  0% {
  	margin-top:15px;
    opacity: 1; }
  100% {
  	margin-top:30px;
    opacity: 0; } }

@include keyframes(dropdownArrowFadeInDown) {
  0% {
    bottom:-5px;
    opacity: 0; }
  100% {
    bottom:-25px;
    opacity: 1; } }

@include keyframes(dropdownArrowFadeOutDown) {
  0% {
    bottom:-25px;
    opacity: 1; }
  100% {
    bottom:-45px;
    opacity: 0; } }


@include keyframes(roleOut) {
  0% {
    max-height: 15px;
  }
  100% {
    max-height: 600px; 
  }
}